<% if types.present? %>
  $("#admin-property-type").html("<%= escape_javascript options_for_select(types) %>");
  <% if selected_value.present? %>
    $('#admin-property-type option[value="' + "<%= selected_value %>" + '"]' ).attr("selected", "true");
  <% end %>
<% else %>
  $("#admin-property-type").html("<%= escape_javascript options_for_select(Property::PROPERTY_TYPES) %>");
<% end %>
