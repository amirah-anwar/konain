<%= link_to APP_CONFIG[:facebook][:profile_link], class: 'fb', target: '_blank', title: 'Facebook' do %>
  <i class='fa fa-facebook'></i>
<% end %>
<%= link_to APP_CONFIG[:twitter][:profile_link], class: 'tw', target: '_blank', title: 'Twitter' do %>
  <i class='fa fa-twitter'></i>
<% end %>
<%= link_to APP_CONFIG[:googleP][:profile_link], class: 'googleP', target: '_blank', title: 'Google+' do %>
  <i class='fa fa-google-plus'></i>
<% end %>
<%= link_to APP_CONFIG[:linkedin][:profile_link], class: 'linkedin', target: '_blank', title: 'Linkedin' do %>
  <i class='fa fa-linkedin'></i>
<% end %>
<%= link_to "skype:#{APP_CONFIG[:skype][:profile_link]}?chat", class: 'skype', target: '_blank', title: 'Skype' do %>
  <i class='fa fa-skype'></i>
<% end %>
